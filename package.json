{
  "name": "scss-to-css",
  "version": "1.0.0",
  "description": "Compiling SCSS into a custom minified CSS file",
  "main": "index.js",
  "scripts": {
    "compress:css": "sass --no-source-map scss/custom.scss:assets/custom.css --style compressed",
    "build": "npm-run-all compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "sass": "^1.52.2"
  }
}
